{
  "name": "react-no-pay",
  "version": "1.0.0",
  "description": "Just a fun package for who dont pay for your work :D",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --watch",
    "dev:website": "turbo run dev --filter=website...",
    "dev:test": "turbo run dev --filter=test...",
    "format": "prettier --write .",
    "test": "playwright test"
  },
  "keywords": [
    "react",
    "nextjs",
    "ui",
    "not-paid",
    "fun",
    "pay"
  ],
  "author": {
    "email": "aminAhmadyDeveloper@gmail.com",
    "name": "Amin Ahmady",
    "url": "https://aminahmady.vercel.app"
  },
  "license": "MIT",
  "homepage": "https://github.com/AminAhmadyDeveloper/react-no-pay",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AminAhmadyDeveloper/react-no-pay.git"
  },
  "bugs": {
    "url": "https://github.com/AminAhmadyDeveloper/react-no-pay/issues"
  },
  "packageManager": "npm@10.2.0",
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "bunchee": "^4.3.3",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
